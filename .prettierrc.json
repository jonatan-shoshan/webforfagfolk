{
  "$schema": "https://json.schemastore.org/prettierrc",
  "endOfLine": "crlf",
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "printWidth": 80,
  "bracketSameLine": true,
  "bracketSpacing": true,
  "arrowParens": "always",
  "trailingComma": "es5",
  "singleQuote": true,
  "singleAttributePerLine": true
}